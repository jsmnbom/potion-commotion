config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="PotionCommotion"
run/main_scene="res://Main/Main.tscn"
config/icon="res://icon.png"

[autoload]

Debug="*res://Singletons/Debug.gd"
Utils="*res://Singletons/Utils.gd"
Data="*res://Singletons/Data.gd"
Events="*res://Singletons/Events.gd"

[debug]

gdscript/warnings/enable=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "zylann.project_stats" )

